{
  "name": "@m3-moretv/react-code-input",
  "version": "1.1.4",
  "description": "Typescript implementation of some code/pin-code input",
  "repository": {
    "type": "git",
    "url": "https://github.com/m3-moretv/react-code-input.git"
  },
  "license": "ISC",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "React",
    "Code Input",
    "PinCode Input",
    "Typescript code input",
    "hooks code input",
    "react-code-input"
  ],
  "author": "Tsareff",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "rm -rf \"lib\" && tsc",
    "test": "jest --config jest.config.json",
    "format": "prettier --write \"src/*.*(js|jsx|ts|tsx)\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@types/classnames": "^2.2.7",
    "@types/react": "^16.8.2",
    "classnames": "^2.2.6",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/enzyme": "^3.1.18",
    "@types/enzyme-adapter-react-16": "^1.0.4",
    "@types/jest": "^24.0.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  }
}
